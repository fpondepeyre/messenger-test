{
    "name": "fpondepeyre/messenger-test",
    "description": "Assertions and helpers for testing your symfony/messenger queues.",
    "homepage": "https://github.com/zenstruck/messenger-test",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["messenger", "symfony", "test", "queue"],
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "symfony/framework-bundle": "^4.4|^5.0",
        "symfony/messenger": "^4.4|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.0",
        "rector/rector": "^0.9.16",
        "symfony/browser-kit": "^4.4|^5.0",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/yaml": "^4.4|^5.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Zenstruck\\Messenger\\Test\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Zenstruck\\Messenger\\Test\\Tests\\": "tests/" }
    }
}
